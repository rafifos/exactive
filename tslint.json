{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-plugin-prettier",
    "tslint-config-prettier"
  ],
  "jsRules": {
    "no-unused-expression": true
  },
  "rules": {
    "member-access": [false],
    "ordered-imports": [true],
    "member-ordering": [false],
    "interface-name": [false],
    "object-literal-sort-keys": false,
    "prettier": true
  },
  "linterOptions": {
    "exclude": ["dist"]
  }
}
